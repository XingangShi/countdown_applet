<!--index.wxml-->
<view class="container" class="container">
  <view class="userinfo">
    <button wx:if="{{!hasUserInfo && canIUse}}" class="userinfo_first" open-type="getUserInfo" bindgetuserinfo="getUserInfo"> 点击获取个性界面 </button>
    <block wx:else>
      <!--<image bindtap="bindViewTap" class="userinfo-avatar" src="{{userInfo.avatarUrl}}" mode="cover"></image>-->
      <image class="userinfo-avatar" src="https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1531226373829&di=d9d060eb04188fc86719745fdb348aae&imgtype=0&src=http%3A%2F%2Fpic1.win4000.com%2Fwallpaper%2Fa%2F59b74bdba14d1.jpg" mode="cover"></image>
      <text class="userinfo-nickname">2019 年</text>
     <text class="userinfo-nickname">{{nickname}}，一起做时间的朋友</text>

      <view class="info_split">
         <text class="info_split"> -------------------------------------------------- </text>
      </view>

      <view class="other_info">
        <button class='recommend_read' bindtap='pmp_exam'>报考PMP</button>
      </view>



      <view class="goddes_value">
           <text class="other_refer">2019 年的 PMP 项目管理认证培训考试时间</text>
      </view>
      <view>

          <text class="time_intro">第一季度考试时间：2019 年 3 月 30 日。\n</text>
          <text class="time_intro">第二季度考试时间：2019 年 6 月 22 日。\n</text>
          <text class="time_intro">第三季度考试时间：2019 年 9 月 7 日。\n</text>
          <text class="time_intro">第四季度考试时间：2019 年 12 月 7 日。\n</text>
      </view>

      <view class="goddes_value">
          <text class="other_refer">2019年注册会计师全国统一考试考试时间</text>
      </view>
      <view>
          <text class="time_intro">2019年8月24日举行综合阶段考试。\n</text>
          <text class="time_intro">2019年10月19-20日举行专业阶段考试。\n</text>
          <text class="time_intro">2019年10月26-27日欧洲考区考试。\n</text>
      </view>

      <view class="goddes_value">
          <text class="other_refer">2019年法律职业资格考试报名时间</text>
      </view>
      <view>
          <text class="time_intro">2019年司法考试客观题考试时间预计为9月下旬。\n</text>
      </view>


      <view class="goddes_value">
           <text class="other_refer">2019年度专业技术人员资格考试工作计划</text>
      </view>
      <view>
           <image src="https://ws1.sinaimg.cn/large/006tNbRwly1fyr5nh4699j30dc2agn27.jpg" class="gzh" mode="cover"></image>
      </view>


      <view class="info_split">
         <text class="info_split"> -------------------------------------------------- </text>
      </view>
      <view class="goddes_value">
           <text class="other_refer">友情链接（网址或公众号点击可复制）</text>
        </view>
        <view>
          <text class="compy_intro">新睿PMP培训官网：</text><text class="fesval_info" selectable='true'>http://www.51pm.cn/</text>
        </view>
        <view>
          <text class="compy_intro">新睿PMP的公众号：</text><text class="fesval_info" selectable='true'>杭州新睿智业有限公司</text>
        </view>
        <view>
           <image src="https://i.imgur.com/kHtwhyj.png" class="xinrui_log" mode="cover"></image>
        </view>
        <view class="other_info">
        <button class='recommend_read' bindtap='return_main'>返回首页</button>
      </view>
    </block>
  </view>

</view>


